# Base URL for the application
VITE_BASE_URL=/

# Application title and description
VITE_APP_TITLE=Soybean Admin
VITE_APP_DESC=A fresh and elegant admin template

# Router history mode
VITE_ROUTER_HISTORY_MODE=history

# Icon configuration
VITE_ICON_PREFIX=icon
VITE_ICON_LOCAL_PREFIX=icon-local

# Backend service configuration - Development environment
VITE_SERVICE_BASE_URL=http://127.0.0.1:9528/v1

VITE_SERVICE_SUCCESS_CODE=0000,200,0
VITE_SERVICE_LOGOUT_CODES=1001,1002
VITE_SERVICE_MODAL_LOGOUT_CODES=1003,1004
VITE_SERVICE_EXPIRED_TOKEN_CODES=1005,1006

# Auth route mode
VITE_AUTH_ROUTE_MODE=dynamic

# Static auth route home
VITE_ROUTE_HOME=home

# Static super role (only used when auth route mode is static)
VITE_STATIC_SUPER_ROLE=super

# Default menu icon
VITE_MENU_ICON=lucide:layout-dashboard

# Other service base URLs (JSON format) - Development environment
VITE_OTHER_SERVICE_BASE_URL={"demo": "http://127.0.0.1:9528/v1", "amisService": "https://m1.apifoxmock.com/m1/3546534-2258203-default", "lowcodeService": "http://127.0.0.1:3000/api/v1"}

# Development configuration - Enable proxy for local development
VITE_HTTP_PROXY=Y
VITE_SOURCE_MAP=Y
VITE_PROXY_LOG=Y

# Storage prefix
VITE_STORAGE_PREFIX=soybean

# Auto detect updates
VITE_AUTOMATICALLY_DETECT_UPDATE=N

# Iconify API provider URL (optional)
# VITE_ICONIFY_URL=

# DevTools launch editor
# VITE_DEVTOOLS_LAUNCH_EDITOR=code
