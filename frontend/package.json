{
  "name": "soybean-admin",
  "type": "module",
  "version": "1.3.14",
  "description": "A fresh and elegant admin template, based on Vue3、Vite6、TypeScript、NaiveUI and UnoCSS. 一个基于Vue3、Vite6、TypeScript、NaiveUI and UnoCSS的清新优雅的中后台模版。",
  "author": {
    "name": "Soybean",
    "email": "soybeanjs@outlook.com",
    "url": "https://github.com/soybeanjs"
  },
  "license": "MIT",
  "homepage": "https://github.com/soybeanjs/soybean-admin",
  "repository": {
    "url": "https://github.com/soybeanjs/soybean-admin.git"
  },
  "bugs": {
    "url": "https://github.com/soybeanjs/soybean-admin/issues"
  },
  "keywords": [
    "Vue3 admin ",
    "vue-admin-template",
    "Vite6",
    "TypeScript",
    "naive-ui",
    "naive-ui-admin",
    "ant-design-vue v4",
    "UnoCSS"
  ],
  "engines": {
    "node": ">=18.20.0",
    "pnpm": ">=8.7.0"
  },
  "scripts": {
    "build": "vite build --mode prod",
    "build:test": "vite build --mode test",
    "cleanup": "sa cleanup",
    "commit": "sa git-commit",
    "commit:zh": "sa git-commit -l=zh-cn",
    "dev": "vite --mode development",
    "dev:prod": "vite --mode prod",
    "gen-route": "sa gen-route",
    "lint": "eslint . --fix",
    "prepare": "simple-git-hooks",
    "preview": "vite preview",
    "release": "sa release",
    "typecheck": "vue-tsc --noEmit --skipLibCheck",
    "update-pkg": "sa update-pkg"
  },
  "dependencies": {
    "@antv/layout": "1.2.14-beta.8",
    "@antv/x6": "^2.18.1",
    "@antv/x6-plugin-clipboard": "^2.1.6",
    "@antv/x6-plugin-dnd": "^2.1.1",
    "@antv/x6-plugin-history": "^2.2.4",
    "@antv/x6-plugin-keyboard": "^2.2.3",
    "@antv/x6-plugin-minimap": "^2.0.7",
    "@antv/x6-plugin-scroller": "^2.0.10",
    "@antv/x6-plugin-selection": "^2.2.2",
    "@antv/x6-plugin-snapline": "^2.1.7",
    "@antv/x6-plugin-transform": "^2.1.8",
    "@better-scroll/core": "2.5.1",
    "@iconify/vue": "5.0.0",
    "@maita/amis-tools": "^0.1.7",
    "@sa/axios": "workspace:*",
    "@sa/color": "workspace:*",
    "@sa/hooks": "workspace:*",
    "@sa/materials": "workspace:*",
    "@sa/utils": "workspace:*",
    "@vueuse/core": "13.3.0",
    "axios": "1.9.0",
    "axios-retry": "4.5.0",
    "clipboard": "2.0.11",
    "dayjs": "1.11.13",
    "defu": "6.1.4",
    "echarts": "5.6.0",
    "json5": "2.2.3",
    "naive-ui": "2.41.1",
    "nprogress": "0.2.0",
    "pinia": "3.0.3",
    "tailwind-merge": "3.3.0",
    "vue": "3.5.16",
    "vue-draggable-plus": "0.6.0",
    "vue-i18n": "11.1.5",
    "vue-router": "4.5.1"
  },
  "devDependencies": {
    "@elegant-router/vue": "0.3.8",
    "@iconify/json": "2.2.347",
    "@iconify/utils": "^2.3.0",
    "@sa/scripts": "workspace:*",
    "@sa/uno-preset": "workspace:*",
    "@soybeanjs/eslint-config": "1.6.1",
    "@types/node": "22.15.30",
    "@types/nprogress": "0.2.3",
    "@unocss/eslint-config": "66.1.4",
    "@unocss/preset-icons": "66.1.4",
    "@unocss/preset-uno": "66.1.4",
    "@unocss/transformer-directives": "66.1.4",
    "@unocss/transformer-variant-group": "66.1.4",
    "@unocss/vite": "66.1.4",
    "@vitejs/plugin-vue": "5.2.4",
    "@vitejs/plugin-vue-jsx": "4.2.0",
    "amis": "6.11.0",
    "consola": "3.4.2",
    "eslint": "9.28.0",
    "eslint-plugin-vue": "10.2.0",
    "kolorist": "1.8.0",
    "prettier-plugin-jsdoc": "^1.3.3",
    "prettier-plugin-json-sort": "^0.0.2",
    "sass": "1.89.1",
    "simple-git-hooks": "2.13.0",
    "tsx": "4.19.4",
    "typescript": "5.8.3",
    "unplugin-icons": "22.1.0",
    "unplugin-vue-components": "28.7.0",
    "vite": "6.3.5",
    "vite-plugin-progress": "0.0.7",
    "vite-plugin-static-copy": "^2.3.1",
    "vite-plugin-svg-icons": "2.0.1",
    "vite-plugin-vue-devtools": "7.7.6",
    "vue-eslint-parser": "10.1.3",
    "vue-tsc": "2.2.10"
  },
  "simple-git-hooks": {
    "commit-msg": "pnpm sa git-commit-verify",
    "pre-commit": "pnpm typecheck && pnpm lint && git diff --exit-code"
  },
  "website": "https://admin.soybeanjs.cn"
}
