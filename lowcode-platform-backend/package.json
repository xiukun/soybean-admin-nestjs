{
  "name": "lowcode-platform-backend",
  "version": "1.0.0",
  "description": "Low-code platform backend service with DDD architecture",
  "author": {
    "name": "ByteByteBrew",
    "email": "wang1571339199@gmail.com"
  },
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:unit": "jest --selectProjects unit",
    "test:integration": "jest --selectProjects integration",
    "test:e2e": "jest --selectProjects e2e",
    "test:performance": "jest --selectProjects performance",
    "test:security": "jest --testPathPattern=security",
    "test:api": "jest --testPathPattern=api",
    "test:auth": "jest --testPathPattern=auth",
    "test:authz": "jest --testPathPattern=authz",
    "test:validation": "jest --testPathPattern=validation",
    "test:migrations": "jest --testPathPattern=migrations",
    "test:seeds": "jest --testPathPattern=seeds",
    "test:db-connection": "jest --testPathPattern=db-connection",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:ci": "jest --coverage --watchAll=false --selectProjects unit integration",
    "test:coverage": "jest --coverage --selectProjects unit integration",
    "import-example": "ts-node scripts/import-example.ts",
    "list-examples": "ts-node scripts/import-example.ts --list",
    "init-db": "ts-node scripts/init-database.ts",
    "init-code-generation": "node scripts/init-code-generation.js",
    "health-check": "node scripts/health-check.js",
    "health-monitor": "node scripts/health-check.js --monitor",
    "dev-setup": "./scripts/dev-setup.sh",
    "update-imports": "node scripts/update-imports.js",
    "validate-aliases": "node scripts/validate-path-aliases.js",
    "check-imports": "tsc --noEmit",
    "check-circular": "madge --circular --extensions ts src/",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:check": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:seed": "ts-node prisma/seed.ts",
    "db:seed": "npm run prisma:seed"
  },
  "dependencies": {
    "@fastify/compress": "^8.0.1",
    "@fastify/cors": "^10.0.1",
    "@fastify/multipart": "^9.0.1",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/static": "^8.0.1",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/cqrs": "^11.0.0",
    "@nestjs/event-emitter": "^2.0.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-fastify": "^11.0.0",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "^11.0.0",
    "@prisma/client": "^6.5.0",
    "@types/cache-manager": "^4.0.6",
    "@types/cls-hooked": "^4.3.9",
    "@types/handlebars": "^4.0.40",
    "@types/uuid": "^10.0.0",
    "cache-manager": "^7.0.1",
    "chokidar": "^3.5.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cls-hooked": "^4.2.2",
    "fastify": "^5.2.2",
    "fs-extra": "^11.3.0",
    "handlebars": "^4.7.8",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "prisma": "^6.5.0",
    "prom-client": "^15.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "ulid": "^3.0.1",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "axios": "^1.10.0",
    "cross-env": "^7.0.3",
    "duplexify": "4.1.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.0",
    "glob": "^10.0.0",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "madge": "^6.0.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.1.0"
  },
  "prisma": {
    "seed": "ts-node --project prisma/tsconfig.json prisma/seed.ts"
  }
}
