{
  "name": "lowcode-platform-backend",
  "version": "1.0.0",
  "description": "Low-code platform backend service with DDD architecture",
  "author": {
    "name": "ByteByteBrew",
    "email": "wang1571339199@gmail.com"
  },
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:unit": "jest --selectProjects unit",
    "test:integration": "jest --selectProjects integration",
    "test:e2e": "jest --selectProjects e2e",
    "test:performance": "jest --selectProjects performance",
    "test:security": "jest --testPathPattern=security",
    "test:api": "jest --testPathPattern=api",
    "test:auth": "jest --testPathPattern=auth",
    "test:authz": "jest --testPathPattern=authz",
    "test:validation": "jest --testPathPattern=validation",
    "test:migrations": "jest --testPathPattern=migrations",
    "test:seeds": "jest --testPathPattern=seeds",
    "test:db-connection": "jest --testPathPattern=db-connection",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:ci": "jest --coverage --watchAll=false --selectProjects unit integration",
    "test:coverage": "jest --coverage --selectProjects unit integration",
    "import-example": "ts-node scripts/import-example.ts",
    "list-examples": "ts-node scripts/import-example.ts --list",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio"
  },
  "dependencies": {
    "@fastify/compress": "^6.0.0",
