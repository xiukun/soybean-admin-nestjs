# Backend Service Environment Variables

# Application
NODE_ENV=development
APP_PORT=9528

# Database Configuration - Backend Schema
DATABASE_URL="postgresql://soybean:soybean@123.@localhost:25432/soybean-admin-nest-backend?schema=backend"
DIRECT_DATABASE_URL="postgresql://soybean:soybean@123.@localhost:25432/soybean-admin-nest-backend?schema=backend"

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=26379
REDIS_PASSWORD=redis@123.
REDIS_DB=1

# JWT Configuration
JWT_SECRET="JWT_SECRET-soybean-admin-nest!@#123."
JWT_EXPIRE_IN=3600
REFRESH_TOKEN_SECRET="REFRESH_TOKEN_SECRET-soybean-admin-nest!@#123."
REFRESH_TOKEN_EXPIRE_IN=7200

# Swagger Documentation
DOC_SWAGGER_ENABLE=true
DOC_SWAGGER_PATH=api-docs

# CORS Configuration
CORS_ENABLED=true
CORS_ORIGIN="http://localhost:9527,http://127.0.0.1:9527,http://localhost:9555,http://127.0.0.1:9555"
CORS_METHODS="GET,HEAD,PUT,PATCH,POST,DELETE,OPTIONS"
CORS_CREDENTIALS=true
CORS_MAX_AGE=3600

# Service Communication
SERVICE_NAME=backend
LOWCODE_PLATFORM_URL=http://localhost:3002
AMIS_BACKEND_URL=http://localhost:9522

# Casbin
CASBIN_MODEL=model.conf

# File Upload
UPLOAD_PATH=./uploads
MAX_FILE_SIZE=10485760

# Logging
LOG_LEVEL=debug
LOG_DIR=./logs